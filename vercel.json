{
  "version": 2,
  "builds": [
    {
      "src": "app.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/(manifest.json|sw.js|favicon.*)",
      "dest": "/public/$1"
    },
    {
      "src": "/",
      "dest": "/app.py"
    },
    {
      "src": "/api/(.*)",
      "dest": "/app.py"
    }
  ]
}