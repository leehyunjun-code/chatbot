{
  "version": 2,
  "builds": [
    {
      "src": "app.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/public/static/$1"  ← 수정!
    },
    {
      "src": "/favicon.ico",
      "dest": "/public/favicon.ico"  ← 수정!
    },
    {
      "src": "/manifest.json",
      "dest": "/public/manifest.json"  ← 수정!
    },
    {
      "src": "/sw.js",
      "dest": "/public/sw.js"  ← 수정!
    },
    {
      "src": "/api/(.*)",
      "dest": "/app.py"
    },
    {
      "src": "/(.*)",
      "dest": "/app.py"
    }
  ]
}